@{
    ViewBag.Title = "分析销售记录";
}
<div class="portlet grid_12">
    <header>
        <h2>条目信息</h2>
    </header>
    <section>
<script src="@Url.Content("~/Scripts/jquery.validate.min.js")" type="text/javascript"></script>
<script src="@Url.Content("~/Scripts/jquery.validate.unobtrusive.min.js")" type="text/javascript"></script>

@using (Html.BeginForm("Analyse", "SalesRecord", FormMethod.Post, new { @class = "form has-validation" }))
{
                    
                    <input type="hidden"  name="AnalyseID" value="@ViewBag.AnalyseID"/>
                    
    
    <div class="clearfix">
                    <label class="form-label no-description" >请选择起始日期<em>*</em></label>  
                    <div class="form-input">
                        <input type="date" id="StartDate" name="StartDate" class="text-box single-line" required="required" data-val-required="起始日期 字段是必需的。"/>
                    </div>
                    </div>


                    <div class="clearfix">
                    <label class="form-label no-description" >请选择结束日期<em>*</em></label>  
                    <div class="form-input">
                        <input type="date" id="EndDate" name="EndDate" class="text-box single-line" required="required" data-val-required="起始日期 字段是必需的。"/>
                    </div>
                    </div>

                    <div class="form-action clearfix">
                        <button class="button" type="submit" data-icon-primary="ui-icon-circle-check">提交</button>
                    
                    <input type="button" value = "返回上一页" onClick="location.href='javascript:history.go(-1);'"/>
    
    </div>
}
    </section>
</div>